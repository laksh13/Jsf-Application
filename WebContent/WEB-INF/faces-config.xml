	<managed-bean>
		<managed-bean-name>actionBeanAnalysis</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.ActionBeanAnalysis</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbaseAccessBean</property-name>
			<property-class>edu.uic.ids517.model.DbaseAccessBean</property-class>
			<value>#{dbaseAccessBean}</value>
		</managed-property>		
		<managed-property>
			<property-name>dataStatisticsBean</property-name>
			<property-class>edu.uic.ids517.model.DataStatisticsBean</property-class>
			<value>#{dataStatisticsBean}</value>
		</managed-property>
		<managed-property>
			<property-name>dataRegressionBean</property-name>
			<property-class>edu.uic.ids517.model.DataRegressionBean</property-class>
