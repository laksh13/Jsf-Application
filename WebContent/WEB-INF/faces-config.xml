		<managed-property>
			<property-name>messageBean</property-name>
			<property-class>edu.uic.ids517.model.MessageBean</property-class>
			<value>#{messageBean}</value>
		</managed-property>
		<managed-property>
			<property-name>dbaseAccessBean</property-name>
			<property-class>edu.uic.ids517.model.DbaseAccessBean</property-class>
			<value>#{dbaseAccessBean}</value>
		</managed-property>
		<managed-property>
			<property-name>dataBean</property-name>
			<property-class>edu.uic.ids517.model.DataBean</property-class>
			<value>#{dataBean}</value>
