<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<managed-bean>
		<managed-bean-name>bmiBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.BmiBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>units</property-name>
			<property-class>java.lang.String</property-class>
			<value>SI</value>
		</managed-property>
		<managed-property>
			<property-name>weight</property-name>
			<property-class>java.lang.String</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
			<property-name>height</property-name>
			<property-class>java.lang.String</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
			<property-name>bmi</property-name>
			<property-class>java.lang.Double</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>bmiPrime</property-name>
			<property-class>java.lang.Double</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>category</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>actionBeanBmi</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.ActionBeanBmi</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbaseAccessBean</property-name>
			<property-class>edu.uic.ids517.model.DbaseAccessBean</property-class>
			<value>#{dbaseAccessBean}</value>
		</managed-property>
		<managed-property>
			<property-name>bmiBean</property-name>
			<property-class>edu.uic.ids517.model.BmiBean</property-class>
			<value>#{bmiBean}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>loanBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.LoanBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>purchaseprice</property-name>
			<property-class>java.lang.String</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
			<property-name>downPayment</property-name>
			<property-class>java.lang.String</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
