			<property-class>java.lang.String</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
			<property-name>term</property-name>
			<property-class>java.lang.String</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
			<property-name>monthlyPayment</property-name>
			<property-class>java.lang.Double</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>totalPayment</property-name>
			<property-class>java.lang.Double</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>totalWithDownPayment</property-name>
			<property-class>java.lang.Double</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>totalInterest</property-name>
			<property-class>java.lang.Double</property-class>
			<value></value>
		</managed-property>
	</managed-bean>

	<managed-bean>
