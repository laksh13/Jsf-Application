		<managed-bean-class>edu.uic.ids517.model.ActionBeanLoan</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbaseAccessBean</property-name>
			<property-class>edu.uic.ids517.model.DbaseAccessBean</property-class>
			<value>#{dbaseAccessBean}</value>
		</managed-property>
		<managed-property>
			<property-name>loanBean</property-name>
			<property-class>edu.uic.ids517.model.LoanBean</property-class>
			<value>#{loanBean}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
