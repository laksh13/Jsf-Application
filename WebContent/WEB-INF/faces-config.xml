	<managed-bean>
		<managed-bean-name>actionBeanLogin</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.ActionBeanLogin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbaseAccessBean</property-name>
			<property-class>edu.uic.ids517.model.DbaseAccessBean</property-class>
			<value>#{dbaseAccessBean}</value>
		</managed-property>
		<managed-property>
			<property-name>dbUserInfo</property-name>
			<property-class>edu.uic.ids517.model.DbUserInfo</property-class>
